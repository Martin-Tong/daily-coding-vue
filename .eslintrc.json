{
    "env": {
        "browser": true,
        "es2020": true
    },
    "globals": {
        "TWEEN": "readonly",
        "COLORS": "writable",
        "jQuery": "readonly",
        "$": "readonly"
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential",
        "plugin:flowtype/recommended"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "parser": "vue-eslint-parser",
    "plugins": [
        "vue",
        "flowtype"
    ],
    "rules": {
    }
}
